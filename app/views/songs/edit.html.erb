<%= form_for @song do |f| %>
    <label> f.label :name </label><br>
    <%= f.text_field :name %><br>

    <label> f.label :artist_id </label><br>
    <%= f.text_field :artist_id %><br>

    <label> f.label :genre_id </label><br>
    <%= f.text_field :genre_id %><br>

    <%= submit_tag "Update Song" %>
<% end %>
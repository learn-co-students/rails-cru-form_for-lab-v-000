<%= form_for(@song) do |f| %>
  <label>Song Name:</label><br>
  <%= f.text_field :name %><br>
  <label>Song Artist:</label><br>
  <!--<%= f.text_field :artist_id %><br>-->
  <%= f.collection_select(:artist_id, Artist.all, :id, :name) %><br>
  <label>Song Genre:</label><br>
  <!--<%= f.text_field :genre_id %><br>-->
  <%= f.collection_select(:genre_id, Genre.all, :id, :name) %><br>

  <%= f.submit "Update Song"%>
<% end %>

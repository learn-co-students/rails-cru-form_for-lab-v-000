<h1>Update Song</h1>

<%= form_for @song do |f| %>

  <%= f.label 'Name:' %>
  <%= f.text_field :name %>
  <br><br>
  <%= f.label 'Artist ID:' %>
  <%= f.text_field :artist_id %>
  <br><br>
  <%= f.label 'Genre ID:' %>
  <%= f.text_field :genre_id %>
  <br><br>
  <%= f.submit "Update Song" %>

<% end %>

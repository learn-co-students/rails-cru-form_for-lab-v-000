<h1>Edit Song Form</h1>
<%= form_for(@song) do |f| %>
<%= f.label :name %>
<%= f.text_field :name, :value => @song.name %>
<%= f.label :artist_id %>
<%= f.text_area :artist_id, :value => @song.artist_id %>
<%= f.label :genre_id %>
<%= f.text_area :genre_id, :value => @song.genre_id %>
<%= f.submit %>
<% end %>

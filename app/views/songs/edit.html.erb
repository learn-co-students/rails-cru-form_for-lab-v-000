<h1>Edit Song Form</h1>

 <%= form_for(@song) do |f| %>
    <%= f.label :name, "Song Name: " %>
    <%= f.text_field :name %>

     <%= f.label :artist_id, "Artist ID: " %>
    <%= f.text_field :artist_id %>

     <%= f.label :genre_id, "Genre ID: " %>
    <%= f.text_field :genre_id %>

     <%= f.submit %>
  <% end %> 

<h1>Edit <%=@song.name %></h1>

  <%= form_for(@song) do |f| %>

    <%=f.label :name%>:
    <%=f.text_field :name, :value => @song.name %><br>

    <%=f.text_field :artist_id, :value => @song.artist_id %>
    <%=f.text_field :genre_id, :value => @song.genre_id%>

    <%=f.submit "Update Song" %>

    <%end%>

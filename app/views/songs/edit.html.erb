<h1>Edit Song</h1>

<%= form_for(@song) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  
  <%= f.label :artist_id, "Artist ID" %>
  <%= f.number_field :artist_id %><br>

  <%= f.label :genre_id, "Genre ID" %>
  <%= f.number_field :genre_id %><br>

  <%= f.submit %>
<% end %>
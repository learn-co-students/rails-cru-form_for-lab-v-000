<h1>Edit Song</h1>

<%= form_for @song do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br/>

  <%= f.label :artist_id, "Artist" %>
  <%= f.collection_select :artist_id, @artists, :id, :name %><br/>

  <%= f.label :genre_id, "Genre" %>
  <%= f.collection_select :genre_id, @genres, :id, :name %><br/>

  <%= f.submit %>
<% end %>

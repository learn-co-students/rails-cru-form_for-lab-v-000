<%= form_for :song, :url => url_for(:controller => 'songs', :action => 'create') do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :artist_id %>
  <%= f.text_field :artist_id %>
  <%= f.label :genre_id %>
  <%= f.text_field :genre_id %>
  <%= f.submit 'Create Song' %>
<% end %>
<%= form_for @song do |f| %>
  <label>Name:</label><br>
  <%= f.text_field :name, placeholder: "Name" %><br>
  <br>

  <label>Artist ID:</label>
  <%= f.text_field :artist_id, placeholder: "Artist ID" %><br>
  <br>

  <label>Genre ID:</label><br>
  <%= f.text_field :genre_id, placeholder: "Genre ID" %><br>
  <br>

  <%= f.submit "Create Song" %>

<% end %>

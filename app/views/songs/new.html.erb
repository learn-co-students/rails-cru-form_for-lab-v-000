<h1>Song#new</h1>
<p>Find me in app/views/songs/new.html.erb</p>

<h1>New Song</h1>
<%= form_for(@song) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %><br>
    <%= f.label :artist_id %>
    <%= f.number_field :artist_id %><br>
    <%= f.label :genre_id %>
    <%= f.number_field :genre_id %><br>
    <%= f.submit %>
<% end %>   
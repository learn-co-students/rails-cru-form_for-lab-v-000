<%= form_for(@song) do |f| %>
  <fieldset>
    <%= f.label :name, "Name:" %>
    <%= f.text_field :name %>
  </fieldset>
  <fieldset>
    <%= f.label :artist_id, "Artist:" %>
    <%= f.text_field :artist_id %>
  </fieldset>
  <fieldset>
    <%= f.label :genre_id, "Genre:" %>
    <%= f.text_field :genre_id %>
  </fieldset>
  <%= f.submit %>
<% end %>

<%= form_for(@song) do |f| %>
  <label>Name:</label><br>
  <%= f.text_field :name %><br>

  <label>Artist ID:</label><br>
  <%= f.text_field :artist_id, id: 'song[artist_id]' %><br>

    <label>Genre ID:</label><br>
  <%= f.text_field :genre_id, id: 'song[genre_id]' %><br>

  <%= f.submit %>
<% end %>
<%= form_for @song do |f| %>
  <p>Create a new song</p>
    <p>
      <%= f.label :name %><br>
      <%= f.text_field :name %>
    </p>
    <p>
      <%= f.label :artist_id %><br>
      <%= f.text_field :artist_id %>
    </p>
    <p>
      <%= f.label :genre_id %><br>
      <%= f.text_field :genre_id %>
    </p>
  <%= f.submit %>

<% end %>

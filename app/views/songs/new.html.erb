<h3>New Song Form</h3>

<%= form_for(@song) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :artist_id %>
  <%= f.text_field :artist_id, type: "number" %>

  <%= f.label :genre_id %>
  <%= f.text_field :genre_id, type: "number" %>

  <%= f.submit "Create Song" %>
<% end %>

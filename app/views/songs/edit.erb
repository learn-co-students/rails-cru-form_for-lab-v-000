<h1>Song Form</h1>

<%= form_for(@song) do |f| %>
  <%= label :name, "Name"%>
    <%= f.text_field :name %> <br>
  <%= label :artist_id, "Artist ID"%>
    <%= f.text_field :artist_id %> <br>
  <%= label :genre_id, "Genre ID"%>
    <%= f.text_field :genre_id %> <br>
  <%= f.submit "Update Song"%>
<% end %>
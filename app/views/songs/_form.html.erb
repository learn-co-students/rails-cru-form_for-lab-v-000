<%= form_for(@song) do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br></br>
 
  <%= f.label :artist_id %><br>
  <%= f.text_field :artist_id %><br>
  <%= f.collection_select :artist_id, Artist.all, :id, :name %><br></br>

  <%= f.label :genre_id%><br>
  <%= f.text_field :genre_id %><br>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %><br></br>
 
  <%= f.submit %>
<% end %>
<h3>Artist Form</h3>

<%= form_tag artists_path do %>
  <label>Artist Name:</label > 
  <br> <%= text_field_tag :"artist[name]" %> 
  <br> <label>Artist Bio: </label> 
  <br> <%= text_field_tag :"artist[bio]" %> 
  <br> <%= hidden_field_tag :authenticity_token, form_authenticity_token %>

<%= submit_tag "Submit Artist" %>
<% end %>
